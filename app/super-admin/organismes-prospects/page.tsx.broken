          code: org.code,
          name: org.name,
          type: org.type,
          groupe: getGroupeFromType(org.type),
          description: org.description,
          city: org.city,
          address: org.address,
          phone: org.phone,
          email: org.email,
          website: org.website,
          niveau_hierarchique: getNiveauHierarchique(org.type),
          est_organisme_principal: isOrganismePrincipalFromType(org.type)
